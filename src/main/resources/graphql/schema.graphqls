scalar Date
type Mutation{
	createClient(clientDto: ClientDto!):String
}

type Query {
    searchSharedKey(key: String): [Client],
    findAll: [Client]
}


type Client {
    user: String,
    name: String,
    email: String,
    phone: String,
    dateAdd: Date,
    startDate: Date,
	endDate: Date
}

input ClientDto {
    user: String,
    name: String,
   	email: String,
    phone: String,
    dateAdd: Date,
    startDate: Date,
	endDate: Date
}

